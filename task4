// This program demonstrates loops and ranges in Kotlin.
fun main() {
    println("Numbers from 1 to 10:")
    for (i in 1..10) { // Loop from 1 to 10
        println(i) // Output numbers 1 to 10
    }

    println("Even numbers from 1 to 20:")
    for (i in 1..20) {
        if (i % 2 == 0) {
            println(i) // Only even numbers printed
        }
    }
}
